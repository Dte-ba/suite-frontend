{{yield}}
{{#if (is-pending model.cerrarEvento)}}
  <div class="ui active inline mini loader"></div>
{{else}}
{{log model.cerrarEvento}}
  {{#if (eq model.cerrarEvento false)}}
    <i class="ui orange time icon"></i> Abierto
  {{else}}
    <i class="ui blue check icon"></i> Finalizado
  {{/if}}
{{/if}}
